{
  "name": "fanuctpp",
  "displayName": "Advanced Fanuc TP",
  "description": "Advanced Language extension for Fanuc Teach Pendant Programming",
  "version": "0.0.1",
  "publisher": "NathanBadanjek",
  "main": "./out/extension.js",
  "scripts": {
    "watch": "tsc -watch -p .",
    "compile": "tsc -p ."
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.16.5",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4"
  },
  "activationEvents": [],
  "engines": {
    "vscode": "^1.95.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NateyJayBee/fanuctpp"
  },
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
	"keywords": [
		"Fanuc",
		"LS",
    "ls",
    "KL",
    "kl",
    "TP"
	],
  "contributes": {
    "languages": [
      {
        "id": "fanuctp_ls",
        "aliases": [
          "Fanuc LS",
          "fanuctp_ls"
        ],
        "extensions": [
          ".ls",
          ".LS"
        ],
        "configuration": "./fanuc_ls_lang_config.json"
      },
      {
        "id": "fanuctp_kl",
        "aliases": [
          "Fanuc KL",
          "fanuctp_kl"
        ],
        "extensions": [
          ".kl",
          ".KL"
        ],
        "configuration": "./fanuc_kl_lang_config.json"
      }
    ],
    "commands": [
      {
        "command": "extension.openLabelView",
        "title": "Open Label View"
      }
    ],
    "grammars": [
      {
        "language": "fanuctp_ls",
        "scopeName": "source.fanuctp_ls",
        "path": "./syntaxes/fanuctp_ls.tmLanguage.json"
      },
      {
        "language": "fanuctp_kl",
        "scopeName": "source.fanuctp_kl",
        "path": "./syntaxes/fanuctp_kl.tmLanguage.json"
      }
    ],
    "themes": [
        {
            "label": "Fanuc Theme",
            "uiTheme": "vs-dark",
            "path": "./themes/fanuctpp_ls.tmTheme.json"
        }
    ]
  },
  "icon": "images/FanucLogo.png"
}
